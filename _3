#include <stdio.h>

int main(void) {
	// your code goes here
	int t;
	scanf("%d", &t);
	for(int i=0; i<t; i++)
	{
	    int a,b;
	    scanf("%d%d", &a, &b);
	    if(a>b)
	    {
	        printf("%d\n", b);
	    }
	    else if(a<b)
	    {
	        printf("%d\n", a);
	    }
	    else if(a==b)
	    {
	        printf("%d\n", a);
	    }
	}
	return 0;
}

